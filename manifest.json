{
    "name": "Bee Hive",
    "version": "1.0",
    "description": "NYT Spelling Bee automated hints tally",
    "manifest_version": 3,
    "author": "Karl Chun",
    "content_scripts": [{
        "matches": ["https://www.nytimes.com/puzzles/spelling-bee"],
        "js": ["BeeHive.js"],
        "runAt" : "document_end",
        "all_frames": true
    }],
    "permissions": [
        "activeTab",
        "scripting"
    ]    
}